<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AboutMeActivity">

    <TextView
        android:id="@+id/AboutMeName"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="155dp"
        android:layout_marginEnd="54dp"
        android:text="Name"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/Namanya"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Namanya"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="155dp"
        android:layout_marginEnd="54dp"
        android:text="Najwa "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/AboutMeName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/AboutMeEmail"
        android:layout_width="80dp"
        android:layout_height="31dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="24dp"
        android:text="Email"
        app:layout_constraintEnd_toStartOf="@+id/Emailnya"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AboutMeName" />

    <TextView
        android:id="@+id/AboutMeBalance"
        android:layout_width="80dp"
        android:layout_height="31dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="60dp"
        android:text="Balance"
        app:layout_constraintEnd_toStartOf="@+id/Balancenya"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AboutMeEmail" />

    <TextView
        android:id="@+id/Balancenya"
        android:layout_width="80dp"
        android:layout_height="31dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="65dp"
        android:text="0.0"
        android:textAlignment="textEnd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/AboutMeBalance"
        app:layout_constraintTop_toBottomOf="@+id/Emailnya" />

    <TextView
        android:id="@+id/Emailnya"
        android:layout_width="156dp"
        android:layout_height="28dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="35dp"
        android:text="najwaf262@gmail.com"
        android:textAlignment="textEnd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/AboutMeEmail"
        app:layout_constraintTop_toBottomOf="@+id/Namanya" />

    <EditText
        android:id="@+id/AboutMeAmount"
        android:layout_width="134dp"
        android:layout_height="42dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="17dp"
        android:ems="10"
        android:fontFamily="sans-serif-light"
        android:hint="Amount"
        android:includeFontPadding="true"
        android:inputType="number"
        app:layout_constraintEnd_toStartOf="@+id/AboutMeTopUp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AboutMeBalance"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/AboutMeTopUp"
        android:layout_width="110dp"
        android:layout_height="43dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="66dp"
        android:text="TOP UP"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/AboutMeAmount"
        app:layout_constraintTop_toBottomOf="@+id/Balancenya"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/AboutMeAccountDetails"
        android:layout_width="159dp"
        android:layout_height="41dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="164dp"
        android:includeFontPadding="true"
        android:text="Account Details"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/AboutMeName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.181" />

    <Button
        android:id="@+id/RegisterRenter"
        android:layout_width="207dp"
        android:layout_height="48dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="66dp"
        android:layout_marginBottom="292dp"
        android:text="Register Renter"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AboutMeAmount"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/inputAddress"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="171dp"
        android:ems="10"
        android:fontFamily="sans-serif-light"
        android:hint="Address"
        android:includeFontPadding="true"
        android:inputType="text"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputName"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/inputPhone"
        android:layout_width="216dp"
        android:layout_height="48dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="135dp"
        android:layout_marginBottom="113dp"
        android:ems="10"
        android:fontFamily="sans-serif-light"
        android:hint="Phone Number"
        android:inputType="phone"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputAddress"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/inputName"
        android:layout_width="189dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="228dp"
        android:ems="10"
        android:fontFamily="sans-serif-light"
        android:hint="Name"
        android:includeFontPadding="true"
        android:inputType="text"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RegisterRenter"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/AboutMeRegisterButton"
        android:layout_width="110dp"
        android:layout_height="43dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="54dp"
        android:text="REGISTER"
        android:textSize="13sp"
        app:iconTint="#670F0F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/AboutMeCancelButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputPhone"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/AboutMeCancelButton"
        android:layout_width="110dp"
        android:layout_height="43dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="54dp"
        android:text="CANCEL"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/AboutMeRegisterButton"
        app:layout_constraintTop_toBottomOf="@+id/inputPhone"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/IsiAddressRenter"
        android:layout_width="206dp"
        android:layout_height="26dp"
        android:layout_marginBottom="176dp"
        android:gravity="start"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputName"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/PhoneNumDisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="244dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="132dp"
        android:text="Phone Number"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IsiAddressRenter"
        app:layout_constraintVertical_bias="0.925"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/NamaRenterDisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        android:layout_marginBottom="42dp"
        android:text="Name"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/IsiAddressRenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AboutMeTopUp"
        tools:visibility="invisible" />


</androidx.constraintlayout.widget.ConstraintLayout>